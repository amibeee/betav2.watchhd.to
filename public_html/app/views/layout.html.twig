		<div class="bgggg2"></div>
		<!-- Navbar -->
		<div class="navbar-wrapper">
			<div class="navbar">
				<div class="logo" onclick="navigateToHome()">
					<img class="logo2" src="{{ base_url() }}assets/assets/icons/navbarlogo.svg" alt="Logo"/>
					<div class="watchhd">WATCHHD</div>
				</div>
				<div class="Navmenu">
					<div class="home{% if uri_segment(1) == '' %} active{% endif %}">

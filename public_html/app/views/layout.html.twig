							<!-- If the user is logged in but not verified, show a prompt to verify -->
							<button class="btn sign" onclick="navigateToVerify()" data-translate="Verify">{{ t('Verify') }}</button>
						{% endif %}
					{% else %}
						<!-- If the user is not logged in, show Login and Sign up -->
						{% if uri_segment(1) != 'login' and uri_segment(1) != 'signup' %}
							<button class="btn log" onclick="navigateToLogin()" data-translate="Login">{{ t('Login') }}</button>
							<button class="btn sign" onclick="navigateToSignUp()" data-translate="Sign Up">{{ t('Sign Up') }}</button>
						{% endif %}
						{% if uri_segment(1) == 'signup' %}
							<button class="btn sign" onclick="navigateToLogin()" data-translate="Login">{{ t('Login') }}</button>
						{% endif %}
						{% if uri_segment(1) == 'login' %}
							<button class="btn sign" onclick="navigateToSignUp()" data-translate="Sign Up">{{ t('Sign Up') }}</button>
						{% endif %}
					{% endif %}
				</div>

				<div class="language" id="language-switcher">
					<img class="vector" src="{{ base_url() }}assets/assets/icons/arrow.svg"/>
					<div class="current-language" id="current-language">EN</div>
					<img class="Flags" id="flag-icon" src="{{ base_url() }}assets/assets/icons/Englishflag.svg"/>
					<div class="dropdown-menu2" id="dropdown-menu">
						<div class="language-option" data-language="EN">
							<img src="{{ base_url() }}assets/assets/icons/Englishflag.svg">English
						</div>
						<div class="language-option" data-language="DE">
							<img src="{{ base_url() }}assets/assets/icons/Germanflag.svg">Deutsch
						</div>
					</div>
				</div>


				<img class="hamburger-button" id="hamburger-button" src="{{ base_url() }}assets/assets/icons/hamburger.svg"/>
				<div class="drawer" id="drawer">
					<div class="drawer-item">
						<a href="{{ base_url() }}" data-translate="Startseite">{{ t('Startseite') }}</a>
					</div>
					<div class="drawer-item">
						<a href="{{ base_url() }}blog" data-translate="News">{{ t('News') }}</a>
					</div>
					<div class="drawer-item">
						<a href="{{ base_url() }}help" data-translate="Hilfe">{{ t('Help') }}</a>
					</div>
					<div class="drawer-item" onclick="window.location.href='https://forum.watchhd.to/'; return false;" data-translate="Forum">
						{{ t('Forum') }}
					</div>
					<div class="drawer-item" onclick="window.location.href='http://xup.in/dl,19741857'; return false;" data-translate="Buy Bitcoins (Tutorial)">
						{{ t('Buy Bitcoins (Tutorial)') }}
					</div>
					{% if current_user.is_user %}
						{% if current_user.two_fa_verified %}
							<div class="drawer-item">
								<a href="{{ base_url() }}account" data-translate="Account">{{ t('Mein Benutzerkonto') }}</a>
							</div>
							<div class="drawer-item" onclick="navigateToLogout()">{{ t('Abmelden') }}</div>
						{% else %}
							<div class="drawer-item">
								<a href="{{ base_url() }}verify_2fa">{{ t('Verify 2FA') }}</a>
							</div>
						{% endif %}
					{% else %}
						<div class="drawer-item">
							<a href="{{ base_url() }}login">{{ t('Login') }}</a>
						</div>
						<div class="drawer-item">
							<a href="{{ base_url() }}signup">{{ t('Benutzerkonto erstellen') }}</a>
						</div>
					{% endif %}
				</div>
			</div>
		</div>
		<div class="container-fluid-home">


			<div class="alert-container">
			{% if messages|length %}
				{% for message in messages %}
					{{ message|raw }}
				{% endfor %}
			{% endif %}

			{% if buli_alert|length %}
				<div class="alert alert-info">
					<center>{{ buli_alert|raw }}</center>
				</div>
			{% endif %}

				{% if (current_user.premium_until-timestamp) < 172800 %}
					<div class="alert alert-warning notification" id='notification2'>
						<span class="close-btn" onclick="closeNotification('notification2')">&times;</span>
						<center>
							<strong>Hinweis:</strong>
							{% if current_user.premium_until > timestamp  %}Dein Abo läuft in
								{% if (current_user.premium_until-timestamp) < 86400 %}
									{{ sum_format(((current_user.premium_until-timestamp)/3600), 0) }}
									Stunden
								{% else %}
									{{ sum_format(((current_user.premium_until-timestamp)/86400), 0) }}
									Tage(n)
								{% endif %}
								ab. Möchtest du es verlängern?{% else %}Dein Abo ist abgelaufen. Jetzt reaktivieren?
							{% endif %}
						</center>
					</div>
				{% endif %}
			</div>
			<!--
			{% if feedbacks|length %}
				{% for feedback in feedbacks %}
					<div class="alert alert-warning"><i class="fa fa-question" aria-hidden="true"></i> <a href="{{ base_url() }}feedback/{{ feedback.id }}">{{ t('Bitte hinterlasse uns kurz eine Rückmeldung zu einem Zahlungsvorgang.') }}</a></div>
				{% endfor %}
			{% endif %}

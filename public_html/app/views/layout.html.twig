					<div class="help{% if uri_segment(1) == 'help' %} active{% endif %}">
						<a href="{{ base_url() }}help" class="navitems" data-translate="Hilfe">{{ t('Hilfe') }}</a>
					</div>
					<div class="forum">
						<a href="https://forum.watchhd.to" target="_blank" data-translate="Forum" class="navitems">{{ t('Forum') }}</a>
					</div>
					<div class="btc">
						<a href="http://xup.in/dl,19741857" target="_blank" data-translate="Bitcoins kaufen (Tutorial)" class="navitems">{{ t('Bitcoins kaufen (Tutorial)') }}</a>

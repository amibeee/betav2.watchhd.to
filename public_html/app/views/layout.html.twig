						<div class="btc">
							<a href="{{ base_url() }}tvguide" data-translate="TV Guide" class="navitems">{{ t('TV_Guide') }}</a>
						</div>
						<div class="btc">
							<a href="{{ base_url() }}tickets" data-translate="Support" class="navitems">{{ t('Support') }}</a>
						</div>
					{% endif %}
				</div>


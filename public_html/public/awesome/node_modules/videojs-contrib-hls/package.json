    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.0",
    "karma": "^0.13.0",
    "karma-browserify": "^4.4.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^1.1.1",
    "karma-detect-browsers": "^2.0.0",
    "karma-firefox-launcher": "^0.1.0",
    "karma-ie-launcher": "^0.2.0",
    "karma-qunit": "^1.2.1",
    "karma-safari-launcher": "^0.1.0",
    "lodash-compat": "^3.10.0",
    "minimist": "^1.2.0",
    "nodemon": "^1.9.1",
    "npm-run-all": "^1.2.0",
    "portscanner": "^1.0.0",
    "qunitjs": "^2.0.1",
    "serve-static": "^1.10.0",
    "shelljs": "^0.5.3",
    "sinon": "1.10.2",
    "uglify-js": "^2.5.0",
    "videojs-standard": "^4.0.3",
    "videojs-contrib-quality-levels": "^2.0.2",
    "watchify": "^3.6.0",
    "webpack": "^1.13.2",
    "videojs-swf": "^5.2.0"
  },
